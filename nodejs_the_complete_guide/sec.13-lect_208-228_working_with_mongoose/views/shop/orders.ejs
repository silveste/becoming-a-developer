<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
          <main>
            <% if (orders.length > 0) { %>
              <ul class="class="cart__item-list">
                <% orders.forEach( order => { %>
                  <li class="cart__item">
                    <h1># <%= order._id %></h1>
                    <ul>
                      <% order.products.forEach( p => { %>
                        <li><%= p.data.title %> (<%= p.quantity %>)</li>
                      <% }); %>
                    </ul>
                  </li>
                <% }) %>
              </ul>
            <% } else { %>
              <h1>Nothing there!</h1>
            <% } %>
          </main>
        </main>
        <%- include('../includes/end.ejs') %>
